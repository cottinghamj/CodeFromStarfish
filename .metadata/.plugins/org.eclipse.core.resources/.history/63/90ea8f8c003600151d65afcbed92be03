package lesson1graphics;

import processing.core.PApplet;


public class Lesson1Graphics extends PApplet {

	public void setup() {
		size(600, 600);
	}

	// Think of draw methods as having a while(true) loop around it
	public void draw() {
		// Call the method that draws the circle
		circle();
	}
	
	public void circle(){
		fill(0,0, 50);
		ellipse(400, 400, 30, 30);
	}
}
