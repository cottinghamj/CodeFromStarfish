package lilMath;

public class lilMath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 6;
		int b = 3;
		
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println("" + a + "/" + b   + " = " +  (a / b) + " R " +  (a % b));
		System.out.println(a % b);
		
		
	}

}
