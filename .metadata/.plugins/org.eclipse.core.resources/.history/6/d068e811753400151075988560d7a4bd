
public class Vars {

	public static void main(String[] args) {
		// type name = value;
		
		int x = 32;    					// stores integers
		double y = 1.12323232323;		// stores decimals
		String a = "This is a string";	// stores Words
		boolean b = true;				// stores either true or false
		char e = 'e';					// stores a single character
		
		
		int health;
		health = 200;
		
		System.out.println(x);
		System.out.println("Player's health is " + health + "!");
		
	}

}
