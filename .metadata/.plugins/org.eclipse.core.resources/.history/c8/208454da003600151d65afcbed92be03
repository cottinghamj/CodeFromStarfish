package lesson1graphics;

import processing.core.PApplet;


public class Lesson1Graphics extends PApplet {

	public void setup() {
		size(600, 600);
	}

	// Think of draw methods as having a while(true) loop around it
	public void draw() {
		// Call the method that draws the circle
		circle();
	}
	
	public void circle(){
		fill(255,90, 0);
		ellipse(400, 400, 300, 300);
	}
	
	public void square(){
		fill(0, 255, 0);
		ellipse(400, 400, 300, 300);
	}
}
